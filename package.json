{
  "name": "nuxt-builderio",
  "version": "0.1.5",
  "description": "An unofficial Nuxt module for Builder.io, a visual headless CMS.",
  "keywords": [
    "nuxt",
    "nuxt-module",
    "nuxt3",
    "builderio",
    "builder.io",
    "cms",
    "nuxt cms",
    "nuxt builderio",
    "headless cms"
  ],
  "repository": "Joepocalyptic/nuxt-builderio",
  "license": "MIT",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/types.d.ts",
      "import": "./dist/module.mjs",
      "require": "./dist/module.cjs"
    }
  },
  "files": [
    "dist",
    "LICENSE",
    "README.md"
  ],
  "scripts": {
    "prepack": "nuxt-module-build",
    "dev": "nuxi dev playground",
    "dev:build": "nuxi build playground",
    "dev:prepare": "nuxt-module-build --stub && nuxi prepare playground",
    "release": "pnpm run lint && pnpm run prepack && changelogen --release && pnpm publish && git push --follow-tags",
    "lint": "eslint ."
  },
  "dependencies": {
    "@builder.io/sdk-vue": "^0.5.9",
    "@nuxt/kit": "^3.7.4",
    "acorn": "^8.10.0",
    "changelogen": "^0.5.4",
    "defu": "^6.1.2",
    "eslint": "^8.46.0",
    "estree-walker": "^3.0.3",
    "knitwork": "^1.0.0",
    "magic-string": "^0.30.3",
    "mlly": "^1.4.0",
    "nuxt": "^3.7.4",
    "ohash": "^1.1.3",
    "pathe": "^1.1.1",
    "ufo": "^1.2.0",
    "unplugin": "^1.4.0"
  },
  "devDependencies": {
    "@nuxt/devtools": "latest",
    "@nuxt/eslint-config": "^0.1.1",
    "@nuxt/module-builder": "^0.4.0",
    "@nuxt/schema": "^3.7.4",
    "@nuxt/test-utils": "^3.7.4",
    "@types/node": "^18.17.3",
    "vitest": "^1.4.0"
  }
}