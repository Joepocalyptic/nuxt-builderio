{
  "name": "nuxt-builderio",
  "version": "0.1.0",
  "description": "An unofficial Nuxt module for Builder.io, a visual headless CMS.",
  "keywords": [
    "nuxt",
    "nuxt-module",
    "nuxt3",
    "builderio",
    "builder.io",
    "cms",
    "nuxt cms",
    "nuxt builderio",
    "headless cms"
  ],
  "repository": "Joepocalyptic/nuxt-builderio",
  "license": "MIT",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/types.d.ts",
      "import": "./dist/module.mjs",
      "require": "./dist/module.cjs"
    }
  },

  "main": "./dist/module.cjs",
  "types": "./dist/types.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "prepack": "nuxt-module-build",
    "dev": "nuxi dev playground",
    "dev:build": "nuxi build playground",
    "dev:prepare": "nuxt-module-build --stub && nuxi prepare playground",
    "release": "pnpm run lint && pnpm run prepack && changelogen --release && pnpm publish && git push --follow-tags",
    "lint": "eslint ."
  },
  "dependencies": {
    "@builder.io/sdk-vue": "^0.5.9",
    "@nuxt/kit": "^3.6.5"
  },
  "devDependencies": {
    "@nuxt/devtools": "latest",
    "@nuxt/eslint-config": "^0.1.1",
    "@nuxt/module-builder": "^0.4.0",
    "@nuxt/schema": "^3.6.5",
    "@nuxt/test-utils": "^3.6.5",
    "@types/node": "^18.17.3",
    "changelogen": "^0.5.4",
    "eslint": "^8.46.0",
    "nuxt": "^3.6.5",
    "vitest": "^0.34.1"
  }
}
