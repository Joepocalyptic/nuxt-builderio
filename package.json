{
  "name": "nuxt-builderio",
  "version": "0.1.6",
  "description": "An unofficial Nuxt module for Builder.io, a visual headless CMS.",
  "keywords": [
    "nuxt",
    "nuxt-module",
    "nuxt3",
    "builderio",
    "builder.io",
    "cms",
    "nuxt cms",
    "nuxt builderio",
    "headless cms",
    "visual cms",
    "page builder"
  ],
  "repository": "Joepocalyptic/nuxt-builderio",
  "license": "MIT",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/types.d.ts",
      "import": "./dist/module.mjs",
      "require": "./dist/module.cjs"
    }
  },
  "files": [
    "dist",
    "LICENSE",
    "README.md"
  ],
  "scripts": {
    "prepack": "nuxt-module-build",
    "dev": "nuxi dev playground",
    "dev:build": "nuxi build playground",
    "dev:prepare": "nuxt-module-build --stub && nuxi prepare playground",
    "release": "pnpm run lint && pnpm run prepack && changelogen --release && pnpm publish && git push --follow-tags",
    "lint": "eslint ."
  },
  "dependencies": {
    "@builder.io/sdk-vue": "^1.1.2",
    "@nuxt/kit": "^3.11.1",
    "acorn": "^8.13.0",
    "changelogen": "^0.5.7",
    "defu": "^6.1.4",
    "eslint": "^8.57.1",
    "estree-walker": "^3.0.3",
    "knitwork": "^1.1.0",
    "magic-string": "^0.30.12",
    "mlly": "^1.7.2",
    "nuxt": "^3.11.1",
    "ohash": "^1.1.4",
    "pathe": "^1.1.2",
    "ufo": "^1.5.4",
    "unplugin": "^1.14.1"
  },
  "devDependencies": {
    "@nuxt/devtools": "latest",
    "@nuxt/eslint-config": "^0.6.0",
    "@nuxt/module-builder": "^0.8.4",
    "@nuxt/schema": "^3.11.1",
    "@nuxt/test-utils": "^3.12.0",
    "@types/node": "^18.19.59",
    "vitest": "^1.6.0"
  }
}